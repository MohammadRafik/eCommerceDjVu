server {
    listen 80;
#    server_name example.org;

    location / {
        
        proxy_pass http://127.0.0.1:8010;
        proxy_set_header Host $host:80;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        
    }
    location /static_for_nginx {
        root /usr/src/app/src;
    }

    location /media {
        root /usr/src/app/src;
    }
}
